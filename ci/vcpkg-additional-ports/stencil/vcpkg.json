{
  "name": "stencil",
  "version": "0.1.20",
  "description": "Cross Platform Resource Embedding",
  "homepage": "https://github.com/ankurvdev/stencil",
  "license": "BSD-3-Clause",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    }
  ],
  "default-features": [
    "runtime"
  ],
  "features": {
    "tool": {
      "description": "Build stencil tool",
      "dependencies": [
        "ankurvdev-embedresource",
        "ankurvdev-lexyacc",
        "cxxopts",
        "fmt",
        "tinyxml2",
        "toml11",
        "tsl-ordered-map"
      ]
    },
    "runtime": {
      "description": "Build with runtime components",
      "dependencies": [
        "boost-beast",
        "boost-url",
        "date",
        "fmt",
        "rapidjson",
        "stduuid",
        {
          "name": "stencil",
          "default-features": false,
          "features": [
            "tool"
          ],
          "host": true
        }
      ]
    },
    "tests": {
      "description": "Build with unittests",
      "dependencies": [
        "dtl",
        "catch2"
      ]
    }
  }
}